const express = require("express");
const router = express.Router();
const {
  homeData,
  getData,
  getById,
  updateData,
  Deletedata,
} = require("../controllers/homeController");
router.post("/createData", contactController, homeData);
router.get("/getData", contactController, getData);
router.getById("/getById", contactController, getById);
router.put("/updataeData", contactController, updateData);
router.delete("/deleteData", contactController, Deletedata);

module.exports = router;
